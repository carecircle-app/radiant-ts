{
    "name":  "radiant-ts",
    "version":  "0.1.0",
    "private":  true,
    "type":  "module",
    "scripts":  {
                    "prebuild":  "node scripts/vercel-precheck.cjs || exit 0",
                    "build":  "next build",
                    "dev":  "npm run dev:local",
                    "start":  "next start",
                    "lint":  "next lint",
                    "postinstall":  "prisma generate || exit 0",
                    "typegen":  "echo skip-typegen",
                    "dev:web":  "next dev -p 3000",
                    "dev:server":  "tsx backend/server.ts",
                    "dev:all":  "concurrently -k -n WEB,API \"npm:dev:web\" \"npm:dev:server\"",
                    "dev:3001":  "next dev -p 3001",
                    "dev:local":  "next dev -H 127.0.0.1 -p 3000",
                    "backup":  "powershell -NoProfile -ExecutionPolicy Bypass -File scripts\\backup.ps1",
                    "prepare":  "husky",
                    "guardrails:precommit":  "node scripts/guardrails.mjs",
                    "fix:mojibake":  "node scripts/fix-mojibake.mjs",
                    "typecheck":  "tsc --noEmit",
                    "format":  "prettier --write .",
                    "build:clean":  "rimraf .next \u0026\u0026 npm run build"
                },
    "devDependencies":  {
                            "@tailwindcss/postcss":  "^4.1.13",
                            "@types/cors":  "^2.8.19",
                            "@types/express":  "^5.0.3",
                            "@types/helmet":  "^0.0.48",
                            "@types/jsonwebtoken":  "^9.0.10",
                            "@types/node":  "^24.3.1",
                            "@types/react":  "19.1.12",
                            "@typescript-eslint/eslint-plugin":  "^8.43.0",
                            "@typescript-eslint/parser":  "^8.43.0",
                            "autoprefixer":  "^10.4.21",
                            "eslint":  "^9.35.0",
                            "eslint-config-next":  "^15.5.2",
                            "eslint-plugin-react":  "^7.37.5",
                            "fast-glob":  "^3.3.3",
                            "husky":  "^9.1.7",
                            "lint-staged":  "^16.1.6",
                            "postcss":  "^8.5.6",
                            "prettier":  "^3.6.2",
                            "prisma":  "^6.15.0",
                            "tailwindcss":  "^3.4.17",
                            "typescript":  "^5.9.2",
                            "typescript-eslint":  "^8.43.0"
                        },
    "dependencies":  {
                         "@headlessui/react":  "^2.2.7",
                         "@heroicons/react":  "^2.2.0",
                         "@prisma/client":  "^6.15.0",
                         "cors":  "^2.8.5",
                         "express":  "^5.1.0",
                         "framer-motion":  "^12.23.12",
                         "helmet":  "^8.1.0",
                         "jsonwebtoken":  "^9.0.2",
                         "next":  "^15.5.2",
                         "next-intl":  "^4.3.7",
                         "nodemailer":  "^7.0.6",
                         "react":  "^19.1.1",
                         "react-dom":  "^19.1.1",
                         "react-use-measure":  "^2.1.7",
                         "stripe":  "^18.5.0"
                     },
    "lint-staged":  {
                        "*.{ts,tsx,js,jsx,json,md,mdx,css,scss}":  [
                                                                       "prettier --write"
                                                                   ]
                    }
}
